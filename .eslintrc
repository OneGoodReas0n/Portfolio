{
   "parser": "babel-eslint",
   "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
   ],
   "plugins": ["prettier", "react"],
   "rules": {
     "prettier/prettier": "error"
   },
   "parserOptions": {
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "env" : {
      "browser": true,
      "node": true
   }
}